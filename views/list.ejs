<table>
    <thead>
      <tr>
        <th>Username</th>
        <th>Name</th>
        <th>Email</th>
        <th>Age</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% for (let i = 0; i < users.length; i++) { %>
        <tr>
          <td><%= users[i].username %></td>
          <td><%= users[i].name %></td>
          <td><%= users[i].email %></td>
          <td><%= users[i].age %></td>
          <td>
            <a href="/users/<%= users[i].id %>/edit">Edit</a>
            <a href="/users/<%= users[i].id %>/delete">Delete</a>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>